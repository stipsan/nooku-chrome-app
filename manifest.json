{
  "manifest_version": 2,
  "name": "Nooku",
  "minimum_chrome_version": "24.0.1307.0",
  "version": "2.0.0",
  "icons": {
    "16": "icon_16.png",
    "128": "icon_128.png"
  },
  "app": {
    "background": {
      "scripts": ["main.js"]
    }
  },
  "permissions": [
    "webview",
    "http://api.mixpanel.com/site_media/js/api/mixpanel.js"
  ],
  "content_security_policy": "script-src 'self' https://example.com 'unsafe-eval'; object-src 'self' 'unsafe-eval'"
}
